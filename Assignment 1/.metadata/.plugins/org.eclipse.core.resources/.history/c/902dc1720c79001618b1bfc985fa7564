public class BasicCounter implements ResetableCounter {
    int value = 0;
    
    public int value() {
		// TODO Auto-generated method stub
        return this.value;
    }

    public void up() {
		// TODO Auto-generated method stub
        ++this.value;
    }

    public void down() {
		// TODO Auto-generated method stub
        --this.value;
    }

    public void reset() {
		// TODO Auto-generated method stub
        this.value = 0;
    }

	/**
	 * @param args
	 */
    public static void main(String[] args) {
		// TODO Auto-generated method stub

    }

}
